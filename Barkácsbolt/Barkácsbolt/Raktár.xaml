        <Button x:Name="Raktar" Content="Raktár" HorizontalAlignment="Left" Margin="21,373,0,0" VerticalAlignment="Top" Height="37" Width="155" Click="Button_Click"/>
        <Button x:Name="UjUgyfel" Content="Új ügyfél regisztrálása" HorizontalAlignment="Left" Margin="223,373,0,0" VerticalAlignment="Top" Height="37" Width="155" Click="Button_Click"/>

